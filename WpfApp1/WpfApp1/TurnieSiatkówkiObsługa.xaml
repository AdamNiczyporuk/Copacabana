<Window x:Class="WpfApp1.TurnieSiatkówkiObsługa"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="TurnieSiatkówkiObsługa" Height="450" Width="800"
        Background="LightBlue">
    <Grid>
        <ListBox d:ItemsSource="{d:SampleData ItemCount=5}" Margin="10,10,432,304"
                 x:Name="ListaMeczy"/>
        <Button Content="Dodaj Spotkanie" HorizontalAlignment="Left" Margin="10,135,0,0" VerticalAlignment="Top"
                Click="DodajMecz"
                Width="150"/>
        <Button Content="Wprowadź Wynik Meczu" HorizontalAlignment="Left" Margin="218,135,0,0" VerticalAlignment="Top"
                Width="150"
                Click="UstawWynikMeczu"/>

    </Grid>
</Window>
